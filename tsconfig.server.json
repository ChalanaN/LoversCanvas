{
    "include": [
        "./*.ts"
    ],
    "exclude": [ "./app/*.ts" ],
    "compilerOptions": {
        "outDir": ".",
        "module": "NodeNext",
        "target": "ES2021",
        "inlineSourceMap": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true
    }
}